<workbook>
	<worksheet name="IT">
		<section startRow="0" endRow="0" />
		<loop startRow="1" endRow="1" items="result" var="ass"
			varType="com.assessment.web.dto.AssessmentExcelDto">
			<section startRow="1" endRow="1">
				<mapping row="1" col="0">ass.jobDesriptionName</mapping>
				<mapping row="1" col="1">ass.jobdesc</mapping>
				
				<mapping row="1" col="2">ass.level1_qualifier1</mapping>
				<mapping row="1" col="3">ass.level1_qualifier2</mapping>
				
				<mapping row="1" col="3">ass.level2_qualifier1</mapping>
				<mapping row="1" col="4">ass.level2_qualifier2</mapping>
				
				<mapping row="1" col="5">ass.level3_qualifier1</mapping>
				<mapping row="1" col="6">ass.level3_qualifier2</mapping>
				
				<mapping row="1" col="7">ass.level4_qualifier1</mapping>
				<mapping row="1" col="8">ass.level4_qualifier2</mapping>
				
				<mapping row="1" col="9">ass.level5_qualifier1</mapping>
				<mapping row="1" col="10">ass.level5_qualifier2</mapping>
			
				
				
				
				<mapping row="1" col="11">ass.level1CourseVideoUrl</mapping>
				<mapping row="1" col="12">ass.level2CourseVideoUrl</mapping>
				<mapping row="1" col="13">ass.level3CourseVideoUrl</mapping>
				<mapping row="1" col="14">ass.level4CourseVideoUrl</mapping>
				<mapping row="1" col="15">ass.level5CourseVideoUrl</mapping>

			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0">End Rows:</cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>